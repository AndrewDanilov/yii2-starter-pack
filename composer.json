{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-openssl": "*",
		"ext-curl": "*",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "rmrevin/yii2-fontawesome": "~3.3",
        "mihaildev/yii2-elfinder": "*@dev",
        "sadovojav/yii2-ckeditor": "dev-master",
        "andrewdanilov/yii2-admin-panel": "dev-master",
        "andrewdanilov/yii2-input-images": "dev-master",
        "andrewdanilov/yii2-thumbs": "dev-master",
        "andrewdanilov/yii2-menu": "dev-master",
        "andrewdanilov/yii2-yandexmap": "dev-master",
        "andrewdanilov/yii2-yandex-metrika": "dev-master",
        "andrewdanilov/yii2-site-years": "dev-master",
        "andrewdanilov/yii2-grecaptchav3": "dev-master",
        "andrewdanilov/yii2-grid-tools": "dev-master",
        "andrewdanilov/yii2-custom-pages": "dev-master"
	},
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "platform": {
            "php": "7.2"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
