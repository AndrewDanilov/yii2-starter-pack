{
    "name": "andrewdanilov/yii2-starter-pack",
    "description": "Yii 2 Advanced Project Template Starter Pack",
    "keywords": ["yii2", "framework", "advanced", "project template", "starter pack"],
    "type": "project",
    "license": "BSD-3-Clause",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-openssl": "*",
		"ext-curl": "*",
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "mihaildev/yii2-elfinder": "*@dev",
        "andrewdanilov/yii2-admin-panel": "~2.0.3"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "platform": {
            "php": "7.2"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
